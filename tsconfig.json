{
  "compilerOptions": {
      "sourceMap": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es6",
      "jsx": "react",
      "removeComments": true,
      "baseUrl": "./",
      "paths": {
        "components/*": [
          "src/components/*"
        ],
      },
      "lib": [
        "es2015",
        "es2017",
        "dom"
      ],
      "experimentalDecorators": true,
  }
}